<% if params[:likeable_type] == 'post'  %>
$(".like-post-<%= params[:likeable_id] %>").html("<%= escape_javascript(like_unlike_link(@post, @current_user, 'post')) %>");
<% else %>
$(".like-comment-<%= params[:likeable_id] %>").html("<%= escape_javascript(like_unlike_link(@comment, @current_user, 'comment')) %>");
<% end %>